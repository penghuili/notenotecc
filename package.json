    "openpgp": "5.11.1",
    "plyr": "3.7.8",
    "qrcode.react": "3.1.0",
    "ramda": "0.30.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "4.0.13",
