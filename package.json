  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "scripts": {
    "time": "sudo ntpdate time.nist.gov",
    "cc": "node scripts/commit.js",
    "cs": "rm -rf src/shared-private && mkdir src/shared-private && cp -a ../sharedjs-private/lib/js src/shared-private/js && cp -a ../sharedjs-private/lib/react src/shared-private/react && rm -f scripts/upload.js && cp -a ../sharedjs-private/scripts/upload.js scripts/upload.js",
    "start": "npm run cs && ESLINT_NO_DEV_ERRORS=true BROWSER=none vite",
    "build": "vite build",
    "dd": "npm run cs && node ./scripts/upload.js",
    "lint": "eslint .",
    "ana": "vite --profile"
  }
}
